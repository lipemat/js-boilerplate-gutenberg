{
  "name": "@lipemat/js-boilerplate-gutenberg",
  "description": "Gutenberg extension for @lipemat/js-boilerplate",
  "version": "1.4.0",
  "license": "MIT",
  "main": "dist/index.js",
  "umd:main": "dist/index.umd.js",
  "module": "dist/index.module.js",
  "jsnext:main": "dist/index.module.js",
  "react-native": "dist/index.module.js",
  "globals": {
    "@wordpress/api-fetch": "wp.apiFetch",
    "@wordpress/url": "wp.url"
  },
  "typings": "dist/index.d.ts",
  "source": "src/index.ts",
  "files": [
    "config/",
    "dist/",
    "helpers/",
    "src/"
  ],
  "private": false,
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lipemat/js-boilerplate-gutenberg.git"
  },
  "bugs": {
    "url": "https://github.com/lipemat/js-boilerplate-gutenberg/issues"
  },
  "homepage": "https://github.com/lipemat/js-boilerplate-gutenberg#readme",
  "dependencies": {
    "@types/classnames": "^2.2.9",
    "@types/js-cookie": "^2.2.4",
    "@types/lipemat-js-boilerplate": "https://github.com/lipemat/types-js-boilerplate.git",
    "@types/tinymce": "^4.5.23",
    "@wordpress/api-fetch": "^3.11.0",
    "@wordpress/i18n": "^3.9.0",
    "@wordpress/url": "^2.15.0",
    "js-cookie": "^2.2.1"
  },
  "devDependencies": {
    "@lipemat/js-boilerplate": ">=5.7.1",
    "@wordpress/api-fetch": "^3.20.0",
    "microbundle": "^0.12.3",
    "unfetch": "^4.1.0"
  },
  "peerDependencies": {
    "@lipemat/js-boilerplate": ">=5.7.1"
  },
  "scripts": {
    "build": "microbundle --external @wordpress/api-fetch,@wordpress/i18n,@wordpress/url --globals @wordpress/api-fetch=wp.apiFetch,@wordpress/i18n=wp.i18n,@wordpress/url=wp.url",
    "prepublish": "rm -rf dist/* && yarn run build",
    "test": "lipemat-js-boilerplate test",
    "watch": "microbundle watch --external @wordpress/api-fetch,@wordpress/i18n,@wordpress/url --globals @wordpress/api-fetch=wp.apiFetch,@wordpress/i18n=wp.i18n,@wordpress/url=wp.url"
  }
}
